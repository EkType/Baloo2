#--------------------------------------------------------------------

languagesystem DFLT dflt;
languagesystem latn dflt;
languagesystem latn NLD;
languagesystem latn AZE;
languagesystem latn TRK;
languagesystem latn MOL;
languagesystem latn ROM;
languagesystem latn CAT;
languagesystem latn TAT;
languagesystem latn CRT;
languagesystem latn KAZ;
languagesystem gjr2 dflt;
languagesystem gujr dflt;



#--------------------------------------------------------------------

#---------- L A T I N ----------#

#--------------------------------------------------------------------

feature aalt { # Access All Alternates
feature locl;
feature subs;
feature sinf;
feature sups;
feature numr;
feature dnom;
feature frac;
feature ordn;
feature tnum;
} aalt;

#--------------------------------------------------------------------

feature frac { # Fractions

lookup FractionBar {
        ignore sub slash @figures @figures @figures @figures @figures @figures @figures @figures @figures @figures slash';
        ignore sub slash' @figures @figures @figures @figures @figures @figures @figures @figures @figures @figures slash;
        ignore sub slash @figures @figures @figures @figures @figures @figures @figures @figures @figures slash';
        ignore sub slash' @figures @figures @figures @figures @figures @figures @figures @figures @figures slash;
        ignore sub slash @figures @figures @figures @figures @figures @figures @figures @figures slash';
        ignore sub slash' @figures @figures @figures @figures @figures @figures @figures @figures slash;
        ignore sub slash @figures @figures @figures @figures @figures @figures @figures slash';
        ignore sub slash' @figures @figures @figures @figures @figures @figures @figures slash;
        ignore sub slash @figures @figures @figures @figures @figures @figures slash';
        ignore sub slash' @figures @figures @figures @figures @figures @figures slash;
        ignore sub slash @figures @figures @figures @figures @figures slash';
        ignore sub slash' @figures @figures @figures @figures @figures slash;
        ignore sub slash @figures @figures @figures @figures slash';
        ignore sub slash' @figures @figures @figures @figures slash;
        ignore sub slash @figures @figures @figures slash';
        ignore sub slash' @figures @figures @figures slash;
        ignore sub slash @figures @figures slash';
        ignore sub slash' @figures @figures slash;
        ignore sub slash @figures slash';
        ignore sub slash' @figures slash;
        ignore sub slash slash';
        ignore sub slash' slash;
        sub @figures slash' @figures by fraction;
    } FractionBar;
    
    lookup Numerator1 {
        sub @figures' fraction by @figures_numerator;
    } Numerator1;

    lookup Numerator2 {
        sub @figures' @figures_numerator fraction by @figures_numerator;
    } Numerator2;

    lookup Numerator3 {
        sub @figures' @figures_numerator @figures_numerator fraction by @figures_numerator;
    } Numerator3;

    lookup Numerator4 {
        sub @figures' @figures_numerator @figures_numerator @figures_numerator fraction by @figures_numerator;
    } Numerator4;

    lookup Numerator5 {
        sub @figures' @figures_numerator @figures_numerator @figures_numerator @figures_numerator fraction by @figures_numerator;
    } Numerator5;

    lookup Numerator6 {
        sub @figures' @figures_numerator @figures_numerator @figures_numerator @figures_numerator @figures_numerator fraction by @figures_numerator;
    } Numerator6;

    lookup Numerator7 {
        sub @figures' @figures_numerator @figures_numerator @figures_numerator @figures_numerator @figures_numerator @figures_numerator fraction by @figures_numerator;
    } Numerator7;

    lookup Numerator8 {
        sub @figures' @figures_numerator @figures_numerator @figures_numerator @figures_numerator @figures_numerator @figures_numerator @figures_numerator fraction by @figures_numerator;
    } Numerator8;

    lookup Numerator9 {
        sub @figures' @figures_numerator @figures_numerator @figures_numerator @figures_numerator @figures_numerator @figures_numerator @figures_numerator @figures_numerator fraction by @figures_numerator;
    } Numerator9;

    lookup Numerator10 {
        sub @figures' @figures_numerator @figures_numerator @figures_numerator @figures_numerator @figures_numerator @figures_numerator @figures_numerator @figures_numerator @figures_numerator fraction by @figures_numerator;
    } Numerator10;

    lookup Denominator {
        sub [fraction @figures_denominator] @figures' by @figures_denominator;
    } Denominator;

    sub @figures space' @figures_numerator by uni2009;

} frac;


#--------------------------------------------------------------------

feature tnum { # Tabular Figures
    sub @figures by @figures_tabular;
} tnum;

#--------------------------------------------------------------------

feature numr { # Numerators
    sub @figures by @figures_numerator;
} numr;

#--------------------------------------------------------------------

feature dnom { # Denominators
    sub @figures by @figures_denominator;
} dnom;

#--------------------------------------------------------------------

feature sups { # Superscript
    sub @for_subscript_superscript by @figures_superscript;
    sub n by n.superior;
} sups;

#--------------------------------------------------------------------

feature subs { # Subscript
    sub @for_subscript_superscript by @figures_subscript;
} subs;

#--------------------------------------------------------------------

feature sinf { # Scientific Inferior
    sub @for_subscript_superscript by @figures_subscript;
} sinf;

#--------------------------------------------------------------------

feature ordn { # Ordinals
  sub @figures [A a]' by ordfeminine;
  sub @figures [O o]' by ordmasculine;
  sub N o period by uni2116;
} ordn;

#--------------------------------------------------------------------

feature liga {
  sub f f i by f_f_i;
  sub f f l by f_f_l;
  sub f f by f_f;
  sub f i by fi;
  sub f l by fl;
} liga;


#--------------------------------------------------------------------


feature locl {
script latn;

language NLD;
  sub iacute j by iacute_j.loclNLD;
  sub Iacute J by Iacute_J.loclNLD;

language CAT;
  sub l periodcentered' l by periodcentered.loclCAT;
  sub L periodcentered' L by periodcentered.loclCAT.case;

language TRK;
  lookup IDOT {
      sub i' by i.loclTRK;
      } IDOT;

language AZE;
  lookup IDOT;
language CRT;
    lookup IDOT;
language KAZ;
    lookup IDOT;
language TAT;
    lookup IDOT;

language ROM;
    lookup CEDILLA {
      sub scedilla by scommaaccent;
      sub Scedilla by Scommaaccent;
    sub uni0162 by uni021A;
    sub uni0163 by uni021B;
    } CEDILLA;
language MOL;
  lookup CEDILLA;

} locl;  


###################################################################

#--------------------------------------------------------------------





#--------------------------------------------------------------------

#---------- G U J A R A T I ----------#

#--------------------------------------------------------------------


feature akhn { # Akhanda
script gjr2;
language dflt;
lookup akhn1 {
	sub Ka.gj Halant.gj Ssa.gj by KaSsa.gj;
	sub Ja.gj Halant.gj Nya.gj by JaNya.gj;
}akhn1;	
script gujr;
language dflt;
lookup akhn1;
} akhn;

#--------------------------------------------------------------------


feature rphf { # Reph forms
script gjr2;
language dflt;
lookup rphf1 {
	sub Ra.gj Halant.gj by Reph.gj;
} rphf1;
script gujr;
language dflt;
lookup rphf1;
} rphf;


#--------------------------------------------------------------------

feature rkrf { # Rakar
	script gjr2;
	language dflt;
	lookup rkrf1 {
		sub Ka.gj Halant.gj Ra.gj by KaRa.gj;
		sub Kha.gj Halant.gj Ra.gj by KhaRa.gj;
		sub Ga.gj Halant.gj Ra.gj by GaRa.gj;
		sub Gha.gj Halant.gj Ra.gj by GhaRa.gj;
		sub Ca.gj Halant.gj Ra.gj by CaRa.gj;
		sub Jha.gj Halant.gj Ra.gj by JhaRa.gj;
		sub Nya.gj Halant.gj Ra.gj by NyaRa.gj;
		sub Nna.gj Halant.gj Ra.gj by NnaRa.gj;
		sub Ta.gj Halant.gj Ra.gj by TaRa.gj;
		sub Tha.gj Halant.gj Ra.gj by ThaRa.gj;
		sub Da.gj Halant.gj Ra.gj by DaRa.gj;
		sub Dha.gj Halant.gj Ra.gj by DhaRa.gj;
		sub Na.gj Halant.gj Ra.gj by NaRa.gj;
		sub Pa.gj Halant.gj Ra.gj by PaRa.gj;
		sub Pha.gj Halant.gj Ra.gj by PhaRa.gj;
		sub Ba.gj Halant.gj Ra.gj by BaRa.gj;
		sub Bha.gj Halant.gj Ra.gj by BhaRa.gj;
		sub Ma.gj Halant.gj Ra.gj by MaRa.gj;
		sub Ya.gj Halant.gj Ra.gj by YaRa.gj;
		sub La.gj Halant.gj Ra.gj by LaRa.gj;
		sub Va.gj Halant.gj Ra.gj by VaRa.gj;
		sub Sha.gj Halant.gj Ra.gj by ShaRa.gj;
		sub Sa.gj Halant.gj Ra.gj by SaRa.gj;
		sub Ssa.gj Halant.gj Ra.gj by SsaRa.gj;
		sub Ha.gj Halant.gj Ra.gj by HaRa.gj;
        sub Lla.gj Halant.gj Ra.gj by LlaRa.gj;
} rkrf1;
} rkrf ;

#--------------------------------------------------------------------


feature blwf {
script gjr2;
language dflt;
	lookup blwf2{
		sub Halant.gj Ra.gj by Vattu.gj;
}blwf2;
script gujr;
language dflt;
	lookup blwf1{
		sub Ra.gj Halant.gj by Vattu.gj;
}blwf1;
}blwf;


#-------------------------------------------------------------------


feature half  {  # Half Forms
script gjr2;
language dflt;
	lookup half_rakar1 {
		sub KaRa.gj Halant.gj by KaRaHalf.gj;
		sub GaRa.gj Halant.gj by GaRaHalf.gj;
		sub GhaRa.gj Halant.gj by GhaRaHalf.gj;
		sub TaRa.gj Halant.gj by TaRaHalf.gj;
		sub DhaRa.gj Halant.gj by DhaRaHalf.gj;
		sub PaRa.gj Halant.gj by PaRaHalf.gj;
		sub BhaRa.gj Halant.gj by BhaRaHalf.gj;
		sub VaRa.gj Halant.gj by VaRaHalf.gj;
		sub ShaRa.gj Halant.gj by ShaRaHalf.gj;
		sub SsaRa.gj Halant.gj by SsaRaHalf.gj;
		sub SaRa.gj Halant.gj by SaRaHalf.gj;
} half_rakar1;

lookup half_rakar2 {
		sub Ka.gj Halant.gj Ra.gj Halant.gj by KaRaHalf.gj;
		sub Ga.gj Halant.gj Ra.gj Halant.gj by GaRaHalf.gj;
		sub Gha.gj Halant.gj Ra.gj Halant.gj by GhaRaHalf.gj;
		sub Ta.gj Halant.gj Ra.gj Halant.gj by TaRaHalf.gj;
		sub Dha.gj Halant.gj Ra.gj Halant.gj by DhaRaHalf.gj;
		sub Pa.gj Halant.gj Ra.gj Halant.gj by PaRaHalf.gj;
		sub Bha.gj Halant.gj Ra.gj Halant.gj by BhaRaHalf.gj;
		sub Va.gj Halant.gj Ra.gj Halant.gj by VaRaHalf.gj;
		sub Sha.gj Halant.gj Ra.gj Halant.gj by ShaRaHalf.gj;
		sub Ssa.gj Halant.gj Ra.gj Halant.gj by SsaRaHalf.gj;
		sub Sa.gj Halant.gj Ra.gj Halant.gj by SaRaHalf.gj;
} half_rakar2;
	
	lookup half1 {
		sub Ka.gj Halant.gj by KaHalf.gj;
		sub Kha.gj Halant.gj by KhaHalf.gj;
		sub Ga.gj Halant.gj by GaHalf.gj;
		sub Gha.gj Halant.gj by GhaHalf.gj;		
		sub Ca.gj Halant.gj by CaHalf.gj;
		sub Ja.gj Halant.gj by JaHalf.gj;
		sub Nya.gj Halant.gj by NyaHalf.gj;
		sub Nna.gj Halant.gj by NnaHalf.gj;
		sub Ta.gj Halant.gj by TaHalf.gj;
		sub Tha.gj Halant.gj by ThaHalf.gj;
		sub Dha.gj Halant.gj by DhaHalf.gj;
		sub Na.gj Halant.gj by NaHalf.gj;
		sub Pa.gj Halant.gj by PaHalf.gj;
		sub Pha.gj Halant.gj by PhaHalf.gj;
		sub Ba.gj Halant.gj by BaHalf.gj;
		sub Bha.gj Halant.gj by BhaHalf.gj;
		sub Ma.gj Halant.gj by MaHalf.gj;
		sub Ya.gj Halant.gj by YaHalf.gj;
		sub La.gj Halant.gj by LaHalf.gj;
		sub Lla.gj Halant.gj by LlaHalf.gj;
		sub Va.gj Halant.gj by VaHalf.gj;
		sub Sha.gj Halant.gj by ShaHalf.gj;
		sub Ssa.gj Halant.gj by SsaHalf.gj;
		sub Sa.gj Halant.gj by SaHalf.gj;
		sub KaSsa.gj Halant.gj by KaSsaHalf.gj;
		sub JaNya.gj Halant.gj by JaNyaHalf.gj;
} half1;
script gujr;
language dflt;
lookup half_rakar1;
lookup half_rakar2; 
lookup half1;
} half;

#--------------------------------------------------------------------

feature vatu { # Vattu
	script gujr;
	language dflt;
	lookup vatu1 {
		sub Ka.gj Vattu.gj by KaRa.gj;
		sub Kha.gj Vattu.gj by KhaRa.gj;
		sub Ga.gj Vattu.gj by GaRa.gj;
		sub Gha.gj Vattu.gj by GhaRa.gj;
		sub Ca.gj Vattu.gj by CaRa.gj;
		sub Jha.gj Vattu.gj by JhaRa.gj;
		sub Nya.gj Vattu.gj by NyaRa.gj;
		sub Nna.gj Vattu.gj by NnaRa.gj;
		sub Ta.gj Vattu.gj by TaRa.gj;
		sub Tha.gj Vattu.gj by ThaRa.gj;
		sub Da.gj Vattu.gj by DaRa.gj;
		sub Dha.gj Vattu.gj by DhaRa.gj;
		sub Na.gj Vattu.gj by NaRa.gj;
		sub Pa.gj Vattu.gj by PaRa.gj;
		sub Pha.gj Vattu.gj by PhaRa.gj;
		sub Ba.gj Vattu.gj by BaRa.gj;
		sub Bha.gj Vattu.gj by BhaRa.gj;
		sub Ma.gj Vattu.gj by MaRa.gj;
		sub Ya.gj Vattu.gj by YaRa.gj;
		sub La.gj Vattu.gj by LaRa.gj;
		sub Va.gj Vattu.gj by VaRa.gj;
		sub Sha.gj Vattu.gj by ShaRa.gj;
		sub Sa.gj Vattu.gj by SaRa.gj;
		sub Ssa.gj Vattu.gj by SsaRa.gj;
		sub Ha.gj Vattu.gj by HaRa.gj;
		sub Lla.gj Vattu.gj by LlaRa.gj;
} vatu1;


	lookup vatu_half1 {
		sub KaHalf.gj Vattu.gj by KaRaHalf.gj;
		sub GaHalf.gj Vattu.gj by GaRaHalf.gj;
		sub GhaHalf.gj Vattu.gj by GhaRaHalf.gj;
		sub TaHalf.gj Vattu.gj by TaRaHalf.gj;
		sub DhaHalf.gj Vattu.gj by DhaRaHalf.gj;
		sub PaHalf.gj Vattu.gj by PaRaHalf.gj;
		sub BhaHalf.gj Vattu.gj by BhaRaHalf.gj;
		sub VaHalf.gj Vattu.gj by VaRaHalf.gj;
		sub ShaHalf.gj Vattu.gj by ShaRaHalf.gj;
		sub SsaHalf.gj Vattu.gj by SsaRaHalf.gj;
		sub SaHalf.gj Vattu.gj by SaRaHalf.gj;		
} vatu_half1;
}vatu;

#--------------------------------------------------------------------


feature cjct { # Pre-base Substitutions
script gjr2;
language dflt;
lookup pres6 { # Four-letter conjuncts
	sub BaHalf.gj SaHalf.gj Tta.gj by BaSaTta.gj;	
	sub NaHalf.gj TaRaHalf.gj Ya.gj by NaTaRaYa.gj;		
} pres6;


lookup pres5 { # Alternate Four-letter conjuncts
	sub NaHalf.gj' SaKaRa.gj by NaHalf_alt.gj;
	sub NaThaHalf.gj' PaRa.gj by NaHalf_alt.gj;
} pres5;
	

lookup pres4 { # Three-letter conjuncts
	sub Da.gj Halant.gj Da.gj Halant.gj Ya.gj by DaDaYa.gj;
	sub KaHalf.gj KaRa.gj by KaKaRa.gj;
	sub KaHalf.gj TaRa.gj by KaTaRa.gj;
	sub KaHalf.gj SaHalf.gj Dda.gj by KaSaDda.gj;
	sub DaRa.gj Halant.gj Ya.gj by DaRaYa.gj;
	sub Da.gj Vattu.gj Halant.gj Ya.gj by DaRaYa.gj;
	sub Da.gj Halant.gj Ra.gj Halant.gj Ya.gj by DaRaYa.gj;
	sub KaHalf.gj SaHalf.gj Tta.gj by KaSaTta.gj;
	sub Cha.gj Vattu.gj Halant.gj Ya.gj by ChaRaYa.gj;
	sub Cha.gj Halant.gj Ra.gj Halant.gj Ya.gj by ChaRaYa.gj;
	sub Tta.gj Vattu.gj Halant.gj Ya.gj by TtaRaYa.gj;
	sub Tta.gj Halant.gj Ra.gj Halant.gj Ya.gj by TtaRaYa.gj;
	sub Da.gj Halant.gj BhaHalf.gj Ya.gj by DaBhaYa.gj;
	sub Da.gj Halant.gj VaHalf.gj Ya.gj by DaVaYa.gj;
	sub Ttha.gj Halant.gj Ttha.gj Halant.gj Ya.gj by TthaTthaYa.gj;
	sub Tta.gj Halant.gj Tta.gj Halant.gj Ya.gj by TtaTtaYa.gj;
	sub TaHalf.gj KaHalf.gj Ya.gj by TaKaYa.gj;
	sub TaHalf.gj KaRa.gj by TaKaRa.gj;
	sub TaHalf.gj KaSsa.gj by TaKaSsa.gj;
	sub TaHalf.gj NaHalf.gj Ya.gj by TaNaYa.gj;

	sub NaHalf.gj KaHalf.gj Sa.gj by NaKaSa.gj;
	sub NaHalf.gj TaRa.gj by NaTaRa.gj;
	sub BaHalf.gj SaHalf.gj Ka.gj by BaSaKa.gj;
	sub LaHalf.gj PhaRa.gj by LaPhaRa.gj;
	sub SsaHalf.gj KaRa.gj by SsaKaRa.gj;
	sub SaHalf.gj KaRa.gj by SaKaRa.gj;
	sub SaHalf.gj TaRa.gj by SaTaRa.gj;
	sub SaHalf.gj ThaHalf.gj Ya.gj by SaThaYa.gj;
	sub LaHalf.gj VaHalf.gj Da.gj by LaVaDa.gj;
	sub LaHalf.gj KaHalf.gj Ya.gj by LaKaYa.gj;	
} pres4;


lookup pres3 { # Alternate Three-letter conjuncts
	sub NaHalf.gj' Sa.gj Ya.gj by NaHalf_alt.gj;
	sub NaHalf.gj' SaKa.gj by NaHalf_alt.gj;
	sub LaHalf.gj' DaRa.gj by LaHalf_alt.gj;
	sub LaHalf.gj' Pa.gj Ya.gj by LaHalf_alt.gj;
	sub LaHalf.gj' ThaHalf.gj Ya.gj by LaHalf_alt2.gj;
} pres3;


lookup pres2 { # Two-letter conjuncts
	sub KaHalf.gj Ka.gj by KaKa.gj;
	sub KaHalf.gj Tha.gj by KaTha.gj;
	sub KaHalf.gj Pha.gj by KaPha.gj;
	sub KaHalf.gj Ma.gj by KaMa.gj;
	sub KaHalf.gj Ya.gj by KaYa.gj;
	sub KaHalf.gj Sha.gj by KaSha.gj;
	sub KaHalf.gj Sa.gj by KaSa.gj;
	sub KhaHalf.gj Sha.gj by KhaSha.gj;
	sub Nga.gj Halant.gj Ka.gj by NgaKa.gj;
	sub Nga.gj Halant.gj Ga.gj by NgaGa.gj;
	sub Nga.gj Halant.gj Ma.gj by NgaMa.gj;
	sub Nga.gj Halant.gj Ya.gj by NgaYa.gj;
	sub CaHalf.gj Ca.gj by CaCa.gj;
	sub Cha.gj Halant.gj Va.gj by ChaVa.gj;
	sub Cha.gj Halant.gj Ya.gj by ChaYa.gj;
	sub JaHalf.gj Ta.gj by JaTa.gj;
	sub Jha.gj Halant.gj Ya.gj by JhaYa.gj;
	sub NyaHalf.gj Ja.gj by NyaJa.gj;
	sub NyaHalf.gj Sha.gj by NyaSha.gj;
	sub Tta.gj Halant.gj Tta.gj by TtaTta.gj;
	sub Tta.gj Halant.gj Ttha.gj by TtaTtha.gj;
	sub Tta.gj Halant.gj Va.gj by TtaVa.gj;
	sub Tta.gj Halant.gj Ya.gj by TtaYa.gj;
	sub Ttha.gj Halant.gj Ttha.gj by TthaTtha.gj;
	sub Ttha.gj Halant.gj Ya.gj by TthaYa.gj;
	sub Dda.gj Halant.gj Ga.gj by DdaGa.gj;
	sub Dda.gj Halant.gj Dda.gj by DdaDda.gj;
	sub Dda.gj Halant.gj Ddha.gj by DdaDdha.gj;
	sub Dda.gj Halant.gj Na.gj by DdaNa.gj;
	sub Dda.gj Halant.gj Ya.gj by DdaYa.gj;
	sub Dda.gj Halant.gj Va.gj by DdaVa.gj;
	sub Ddha.gj Halant.gj Ddha.gj by DdhaDdha.gj;
	sub Ddha.gj Halant.gj Ya.gj by DdhaYa.gj;
	sub TaHalf.gj Ka.gj by TaKa.gj;
	sub TaHalf.gj Ta.gj by TaTa.gj;
	sub TaHalf.gj Pha.gj by TaPha.gj;
	sub NaHalf.gj Ga.gj by NaGa.gj;
	sub NaHalf.gj Ca.gj by NaCa.gj;
	sub NaHalf.gj Tha.gj by NaTha.gj;
	sub NaHalf.gj Na.gj by NaNa.gj;
	sub NaHalf.gj Ma.gj by NaMa.gj;
	sub PaHalf.gj Ka.gj by PaKa.gj;
	sub PaHalf.gj Ca.gj by PaCa.gj;
	sub PaHalf.gj Cha.gj by PaCha.gj;
	sub PaHalf.gj Dda.gj by PaDda.gj;
	sub PaHalf.gj Pha.gj by PaPha.gj;
	sub PaHalf.gj Ma.gj by PaMa.gj;
	sub PaHalf.gj Sha.gj by PaSha.gj;
	sub PaHalf.gj Sa.gj by PaSa.gj;
	sub PhaHalf.gj Tha.gj by PhaTha.gj;
	sub PhaHalf.gj Pha.gj by PhaPha.gj;
	sub PhaHalf.gj Ya.gj by PhaYa.gj;
	sub PhaHalf.gj Sha.gj by PhaSha.gj;
	sub PhaHalf.gj Sa.gj by PhaSa.gj;
	sub BaHalf.gj Dda.gj by BaDda.gj;
	sub BaHalf.gj Da.gj by BaDa.gj;
	sub BaHalf.gj Dha.gj by BaDha.gj;
	sub BaHalf.gj Sha.gj by BaSha.gj;
	sub BaHalf.gj Ha.gj by BaHa.gj;
	sub MaHalf.gj Ka.gj by MaKa.gj;
	sub MaHalf.gj Da.gj by MaDa.gj;
	sub MaHalf.gj Pha.gj by MaPha.gj;
	sub MaHalf.gj Sha.gj by MaSha.gj;
	sub YaHalf.gj Jha.gj by YaJha.gj;
	sub LaHalf.gj Ka.gj by LaKa.gj;
	sub LaHalf.gj Ga.gj by LaGa.gj;
	sub LaHalf.gj Ca.gj by LaCa.gj;
	sub LaHalf.gj Pha.gj by LaPha.gj;
	sub VaHalf.gj Jha.gj by VaJha.gj;
	sub VaHalf.gj Dda.gj by VaDda.gj;
	sub VaHalf.gj Ha.gj by VaHa.gj;
	sub ShaHalf.gj Ca.gj by ShaCa.gj;
	sub ShaHalf.gj Na.gj by ShaNa.gj;
	sub ShaHalf.gj Va.gj by ShaVa.gj;
	sub SsaHalf.gj Ka.gj by SsaKa.gj;
