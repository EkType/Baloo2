	sub Ja.gj' MatraIi.gj' RephBindu.gj by JaMatraIi.gj;
	sub Ja.gj' MatraA.gj' Chandrabindu.gj by JaMatraAa.gj;
	sub Ja.gj' MatraIi.gj' Chandrabindu.gj by JaMatraIi.gj;
