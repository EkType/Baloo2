feature psts { 
script gjr2;
language dflt;
lookup psts3 { # Postbase matra substitutions
	sub @Width_matraIione @matraIidefault' by @matraIione;
	sub @Width_matraIitwo @matraIidefault' by @matraIitwo;
	sub @Width_matraIithree @matraIidefault' by @matraIithree;
	sub @Width_matraIione Vattu.gj @matraIidefault' by @matraIione;
	sub @Width_matraIitwo Vattu.gj @matraIidefault' by @matraIitwo;
	sub @Width_matraIithree Vattu.gj @matraIidefault' by @matraIithree;
} psts3;

lookup psts2 {
	sub Ja.gj MatraIi.gj by JaMatraIi.gj;
	sub Ja.gj MatraA.gj by JaMatraAa.gj;
	sub Ja.gj MatraChandraO.gj by JaMatraChandraO.gj;
	sub Ja.gj MatraO.gj by JaMatraO.gj;
	sub Ja.gj MatraAu.gj by JaMatraAu.gj;
	sub Ja.gj MatraOBindu.gj by JaMatraOBindu.gj;
	sub Ja.gj MatraOReph.gj by JaMatraOReph.gj;
	sub Ja.gj MatraORephBindu.gj by JaMatraORephBindu.gj;
	sub Ja.gj MatraORephChandrabindu.gj by JaMatraORephChandrabindu.gj;
	sub Ja.gj MatraAuBindu.gj by JaMatraAuBindu.gj;
	sub Ja.gj MatraAuReph.gj by JaMatraAuReph.gj;
	sub Ja.gj MatraAuRephBindu.gj by JaMatraAuRephBindu.gj;
	sub Ja.gj MatraAuRephChandrabindu.gj by JaMatraAuRephChandrabindu.gj;
} psts2;


@forChandrabindu1 =[Kha.gj Ga.gj Gha.gj Ca.gj Jha.gj Nya.gj Nna.gj Ta.gj Tha.gj Da.gj Dha.gj Na.gj Pa.gj Ba.gj Bha.gj Ma.gj Ya.gj Ra.gj La.gj Lla.gj Va.gj Sha.gj Ssa.gj Sa.gj KhaRa.gj GaRa.gj GhaRa.gj CaRa.gj JhaRa.gj NyaRa.gj NnaRa.gj TaRa.gj ThaRa.gj DaRa.gj DhaRa.gj NaRa.gj PaRa.gj BaRa.gj BhaRa.gj MaRa.gj YaRa.gj LaRa.gj LlaRa.gj VaRa.gj ShaRa.gj SsaRa.gj SaRa.gj KaTha.gj KaMa.gj KaYa.gj KaSha.gj KaSsa.gj KaSa.gj KhaSha.gj CaCa.gj ChaYa.gj JaNya.gj JaTa.gj JhaYa.gj NyaSha.gj TtaYa.gj TthaYa.gj DdaYa.gj DdhaYa.gj TaTa.gj DaGa.gj DaGha.gj DaDa.gj DaDha.gj DaNa.gj DaBa.gj DaBha.gj DaVa.gj NaGa.gj NaCa.gj NaTha.gj NaNa.gj NaMa.gj PaCa.gj PaMa.gj PaSha.gj PaSa.gj PhaTha.gj PhaYa.gj PhaSha.gj PhaSa.gj BaDa.gj BaDha.gj BaSha.gj MaDa.gj MaSha.gj YaJha.gj LaGa.gj LaCa.gj VaJha.gj ShaCa.gj ShaNa.gj ShaVa.gj SaTha.gj HaMa.gj HaYa.gj KaTaRa.gj ChaRaYa.gj TtaRaYa.gj TtaTtaYa.gj TthaTthaYa.gj DdaDdaYa.gj TaKaYa.gj TaKaSsa.gj TaNaYa.gj DaDaYa.gj DaBhaYa.gj DaVaYa.gj DaRaYa.gj NaKaSa.gj NaTaRa.gj NaTaRaYa.gj LaKaYa.gj LaVaDa.gj SaTaRa.gj SaThaYa.gj DdaDda.gj DaMa.gj DaYa.gj];

@forChandrabindu3.gj =[Ka.gj Nga.gj Cha.gj Tta.gj Ttha.gj Dda.gj Ddha.gj Pha.gj Ha.gj KaRa.gj PhaRa.gj HaRa.gj KaKa.gj NgaKa.gj NgaGa.gj NgaMa.gj NgaYa.gj ChaVa.gj TtaTta.gj TtaTtha.gj TtaVa.gj TthaTtha.gj DdaGa.gj DdaDda.gj DdaDdha.gj DdaNa.gj DdaVa.gj DdhaDdha.gj TaKa.gj TaPha.gj PaKa.gj PaCha.gj PaDda.gj PaPha.gj PhaPha.gj BaDda.gj BaHa.gj MaKa.gj MaPha.gj LaKa.gj LaPha.gj VaDda.gj VaHa.gj SsaKa.gj SsaPha.gj SaKa.gj SaPha.gj HaNna.gj HaNa.gj HaLa.gj HaVa.gj KaKaRa.gj KaSaDda.gj KaSaTta.gj TaKaRa.gj BaSaKa.gj BaSaTta.gj LaPhaRa.gj SsaKaRa.gj SaKaRa.gj];


lookup ChandrabinduReplace {

	sub @MatraI @forChandrabindu1 Chandrabindu.gj' by Chandrabindu1.gj;
	sub @MatraI @allhalfforms @forChandrabindu1 Chandrabindu.gj' by Chandrabindu1.gj;
	sub @MatraI @allhalfforms @allhalfforms @forChandrabindu1 Chandrabindu.gj' by Chandrabindu1.gj;
	sub [VowelChadraE.gj VowelE.gj VowelChandraO.gj VowelO.gj] Chandrabindu.gj' by Chandrabindu1.gj;

	sub @MatraI @forChandrabindu3.gj Chandrabindu.gj' by Chandrabindu3.gj;
	sub @MatraI @allhalfforms @forChandrabindu3.gj Chandrabindu.gj' by Chandrabindu3.gj;
	sub @MatraI @allhalfforms @allhalfforms @forChandrabindu3.gj Chandrabindu.gj' by Chandrabindu3.gj;

	sub @MatraI @forChandrabindu3.gj [Vattu.gj Vattu1.gj] Chandrabindu.gj' by Chandrabindu3.gj;
	sub @MatraI @allhalfforms @forChandrabindu3.gj [Vattu.gj Vattu1.gj] Chandrabindu.gj' by Chandrabindu3.gj;
	sub @MatraI @allhalfforms @allhalfforms @forChandrabindu3.gj [Vattu.gj Vattu1.gj] Chandrabindu.gj' by Chandrabindu3.gj;

	sub @MatraI [Ja.gj NyaJa.gj] Chandrabindu.gj' by Chandrabindu2.gj;
	sub @MatraI @allhalfforms [Ja.gj NyaJa.gj] Chandrabindu.gj' by Chandrabindu2.gj;
	sub @MatraI @allhalfforms @allhalfforms [Ja.gj NyaJa.gj] Chandrabindu.gj' by Chandrabindu2.gj;
	sub [VowelAu.gj VowelAi.gj] Chandrabindu.gj' by Chandrabindu2.gj;

} ChandrabinduReplace;

script gujr;
language dflt;
lookup psts3;
lookup psts2;
lookup ChandrabinduReplace;
} psts;

