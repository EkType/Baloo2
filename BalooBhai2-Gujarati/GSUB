 sub MatraI21RephBindu.gj LaHalf.gj LaHalf.gj Ya.gj RephBindu.gj' by dummy1 ;
 sub MatraI21RephBindu.gj SsaHalf.gj NnaHalf.gj Ya.gj RephBindu.gj' by dummy1 ;
 sub MatraI22RephBindu.gj KaHalf.gj TaHalf_alt.gj Ya.gj RephBindu.gj' by dummy1 ;
 sub MatraI22RephBindu.gj KaHalf.gj LaHalf.gj Ya.gj RephBindu.gj' by dummy1 ;
 sub MatraI22RephBindu.gj BaHalf.gj LaHalf.gj Ya.gj RephBindu.gj' by dummy1 ;
 sub MatraI24RephBindu.gj BaHalf_alt.gj JaHalf_alt.gj Ya.gj RephBindu.gj' by dummy1 ;
 sub MatraI24RephBindu.gj JaHalf_alt.gj JaHalf_alt.gj Va.gj RephBindu.gj' by dummy1 ;
 sub MatraI25RephBindu.gj JaHalf_alt.gj JaHalf_alt.gj Ya.gj RephBindu.gj' by dummy1 ;
 sub MatraI26RephBindu.gj KaSaHalf.gj PaHalf.gj La.gj RephBindu.gj' by dummy1 ;

 sub MatraI21RephBindu.gj DaBhaYa.gj RephBindu.gj' by dummy1 ;
 sub MatraI22RephBindu.gj LaKaYa.gj RephBindu.gj' by dummy1 ;
 
 sub MatraI23RephBindu.gj NaHalf_alt.gj SaHalf.gj MaHalf.gj Ya.gj RephBindu.gj' by dummy1 ;
 sub MatraI25RephBindu.gj NaHalf_alt2.gj PhaHalf.gj LaHalf.gj Ya.gj RephBindu.gj' by dummy1 ;
} MatraIRephBindudummy ;    
     

script gujr;
language dflt;
lookup abvs4;
lookup abvs3;
lookup MatraShort;
lookup abvs2;
lookup abvs1;
lookup MatraIBindu;
lookup MatraIBinduVattu;
lookup MatraIBindudummyVattu;
lookup MatraIBindudummy;
lookup MatraIReph;
lookup MatraIRephVattu;
lookup MatraIRephdummyVattu;
lookup MatraIRephdummy; 
lookup MatraIRephBindu;
lookup MatraIRephBinduVattu;
lookup MatraIRephBindudummyVattu;
lookup MatraIRephBindudummy;
     

} abvs;

