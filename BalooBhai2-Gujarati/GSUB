feature blws { # Belowbase substitution
script gjr2;
language dflt;
lookup blws1 {
	sub Ra.gj MatraU.gj by RaMatraU.gj;
	sub Ra.gj MatraUu.gj by RaMatraUu.gj;
	sub Ha.gj MatraRu.gj by HaRu.gj;
	sub Da.gj MatraRu.gj by DaMatraRu.gj;
	sub KaSsa.gj MatraU.gj by KaSsaMatraU.gj;
	sub JaNya.gj MatraU.gj by JaNyaMatraU.gj;

} blws1;

lookup blws2 {
		sub NgaKa.gj Vattu.gj' by Vattu1.gj;
		sub NgaGa.gj Vattu.gj' by Vattu1.gj;
		sub NgaMa.gj Vattu.gj' by Vattu1.gj;
		sub NgaYa.gj Vattu.gj' by Vattu1.gj;
		sub TtaTta.gj Vattu.gj' by Vattu1.gj;
		sub TtaTtha.gj Vattu.gj' by Vattu1.gj;
		sub TtaVa.gj Vattu.gj' by Vattu1.gj;
		sub TthaTtha.gj Vattu.gj' by Vattu1.gj;
		sub DdaGa.gj Vattu.gj' by Vattu1.gj;
		sub DdaDda.gj Vattu.gj' by Vattu1.gj;
		sub DdaDdha.gj Vattu.gj' by Vattu1.gj;
		sub DdaNa.gj Vattu.gj' by Vattu1.gj;
		sub DdaVa.gj Vattu.gj' by Vattu1.gj;
		sub DdhaDdha.gj Vattu.gj' by Vattu1.gj;
} blws2;

lookup blws_Vattumatra {
	sub Vattu.gj Halant.gj by VattuHalant.gj;
	sub Vattu.gj MatraU.gj by VattuMatraU.gj;
	sub Vattu.gj MatraUu.gj by VattuMatraUu.gj;
	sub Vattu1.gj Halant.gj by VattuHalant1.gj;
	sub Vattu1.gj MatraU.gj by VattuMatraU1.gj;
	sub Vattu1.gj MatraUu.gj by VattuMatraUu1.gj;

} blws_Vattumatra;

lookup blws_Vertical_Conjuncts {
	sub @Vertical_Conjuncts Vattu.gj' by Vattu1.gj;
	sub @Vertical_Conjuncts MatraU.gj' by MatraU1.gj;
	sub @Vertical_Conjuncts MatraUu.gj' by MatraUu1.gj;
	sub @Vertical_Conjuncts MatraRu.gj' by MatraRu1.gj;
} blws_Vertical_Conjuncts; 

script gujr;
language dflt;
lookup blws1;
lookup blws2;
lookup blws_Vattumatra;
lookup blws_Vertical_Conjuncts;
} blws;

